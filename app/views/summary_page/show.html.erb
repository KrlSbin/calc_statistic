  <tr><td><b>Top 15 highest Clients Revenue</b></td><td></td></tr>
  <tr><td></td><td></td></tr>
  <tr><td>Full Name</td><td>Revenue</td></tr>
  <% @top_highest_revenue.each do |client| %>
    <tr><td><%= client.full_name %></td><td><%= client.revenue %></td></tr>
  <% end %>
</table>
</body>
</html>
